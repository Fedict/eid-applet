			<scope>test</scope>
		</dependency>
	</dependencies>
	<repositories>
		<repository>
			<id>e-contract</id>
			<url>http://www.e-contract.be/maven2/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>
</project>
